{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "formulaone-project"
		},
		"ls_formula_one_storage_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'ls_formula_one_storage'"
		},
		"ls_ergast_api_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://ergast.com/"
		},
		"ls_formula_one_storage_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://formuladlstorage1.dfs.core.windows.net/"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "[concat(parameters('factoryName'), '/ls_ergast_api')]",
			"type": "Microsoft.DataFactory/factories/linkedServices",
			"apiVersion": "2018-06-01",
			"properties": {
				"annotations": [],
				"type": "HttpServer",
				"typeProperties": {
					"url": "[parameters('ls_ergast_api_properties_typeProperties_url')]",
					"enableServerCertificateValidation": true,
					"authenticationType": "Anonymous"
				}
			},
			"dependsOn": []
		},
		{
			"name": "[concat(parameters('factoryName'), '/ls_formula_one_storage')]",
			"type": "Microsoft.DataFactory/factories/linkedServices",
			"apiVersion": "2018-06-01",
			"properties": {
				"annotations": [],
				"type": "AzureBlobFS",
				"typeProperties": {
					"url": "[parameters('ls_formula_one_storage_properties_typeProperties_url')]",
					"accountKey": {
						"type": "SecureString",
						"value": "[parameters('ls_formula_one_storage_accountKey')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "[concat(parameters('factoryName'), '/ds_https_seasons_data')]",
			"type": "Microsoft.DataFactory/factories/datasets",
			"apiVersion": "2018-06-01",
			"properties": {
				"linkedServiceName": {
					"referenceName": "ls_ergast_api",
					"type": "LinkedServiceReference"
				},
				"folder": {
					"name": "formula_one_datasets/raw"
				},
				"annotations": [],
				"type": "Json",
				"typeProperties": {
					"location": {
						"type": "HttpServerLocation",
						"relativeUrl": "api/f1/seasons.json?limit=100"
					}
				},
				"schema": {}
			},
			"dependsOn": [
				"[concat(variables('factoryId'), '/linkedServices/ls_ergast_api')]"
			]
		},
		{
			"name": "[concat(parameters('factoryName'), '/ds_raw_drivers')]",
			"type": "Microsoft.DataFactory/factories/datasets",
			"apiVersion": "2018-06-01",
			"properties": {
				"linkedServiceName": {
					"referenceName": "ls_formula_one_storage",
					"type": "LinkedServiceReference"
				},
				"parameters": {
					"season": {
						"type": "String"
					}
				},
				"folder": {
					"name": "formula_one_datasets/raw"
				},
				"annotations": [],
				"type": "DelimitedText",
				"typeProperties": {
					"location": {
						"type": "AzureBlobFSLocation",
						"fileName": {
							"value": "@concat(dataset().season,'_drivers.json')",
							"type": "Expression"
						},
						"folderPath": "bronze/drivers",
						"fileSystem": "bronze"
					},
					"columnDelimiter": ",",
					"escapeChar": "",
					"firstRowAsHeader": false,
					"quoteChar": ""
				},
				"schema": []
			},
			"dependsOn": [
				"[concat(variables('factoryId'), '/linkedServices/ls_formula_one_storage')]"
			]
		},
		{
			"name": "[concat(parameters('factoryName'), '/ds_raw_drivers_temp')]",
			"type": "Microsoft.DataFactory/factories/datasets",
			"apiVersion": "2018-06-01",
			"properties": {
				"linkedServiceName": {
					"referenceName": "ls_formula_one_storage",
					"type": "LinkedServiceReference"
				},
				"folder": {
					"name": "formula_one_datasets/raw"
				},
				"annotations": [],
				"type": "DelimitedText",
				"typeProperties": {
					"location": {
						"type": "AzureBlobFSLocation",
						"fileName": "temp.txt",
						"folderPath": "bronze/temp",
						"fileSystem": "bronze"
					},
					"columnDelimiter": ",",
					"escapeChar": "\\",
					"firstRowAsHeader": false,
					"quoteChar": "\""
				},
				"schema": [
					{
						"name": "abcd",
						"type": "String"
					}
				]
			},
			"dependsOn": [
				"[concat(variables('factoryId'), '/linkedServices/ls_formula_one_storage')]"
			]
		},
		{
			"name": "[concat(parameters('factoryName'), '/ds_raw_races')]",
			"type": "Microsoft.DataFactory/factories/datasets",
			"apiVersion": "2018-06-01",
			"properties": {
				"linkedServiceName": {
					"referenceName": "ls_formula_one_storage",
					"type": "LinkedServiceReference"
				},
				"parameters": {
					"season": {
						"type": "String"
					}
				},
				"folder": {
					"name": "formula_one_datasets/raw"
				},
				"annotations": [],
				"type": "DelimitedText",
				"typeProperties": {
					"location": {
						"type": "AzureBlobFSLocation",
						"fileName": {
							"value": "@concat(dataset().season,'_races.json')",
							"type": "Expression"
						},
						"folderPath": "bronze/races",
						"fileSystem": "bronze"
					},
					"columnDelimiter": ",",
					"escapeChar": "\\",
					"firstRowAsHeader": false,
					"quoteChar": "\""
				},
				"schema": []
			},
			"dependsOn": [
				"[concat(variables('factoryId'), '/linkedServices/ls_formula_one_storage')]"
			]
		},
		{
			"name": "[concat(parameters('factoryName'), '/ds_raw_results')]",
			"type": "Microsoft.DataFactory/factories/datasets",
			"apiVersion": "2018-06-01",
			"properties": {
				"linkedServiceName": {
					"referenceName": "ls_formula_one_storage",
					"type": "LinkedServiceReference"
				},
				"parameters": {
					"season": {
						"type": "String"
					},
					"round": {
						"type": "String"
					}
				},
				"folder": {
					"name": "formula_one_datasets/raw"
				},
				"annotations": [],
				"type": "DelimitedText",
				"typeProperties": {
					"location": {
						"type": "AzureBlobFSLocation",
						"fileName": {
							"value": "@concat(dataset().round,'_result.json')",
							"type": "Expression"
						},
						"folderPath": {
							"value": "@concat('bronze/results/season',dataset().season)",
							"type": "Expression"
						},
						"fileSystem": "bronze"
					},
					"columnDelimiter": ",",
					"escapeChar": "",
					"firstRowAsHeader": false,
					"quoteChar": ""
				},
				"schema": []
			},
			"dependsOn": [
				"[concat(variables('factoryId'), '/linkedServices/ls_formula_one_storage')]"
			]
		},
		{
			"name": "[concat(parameters('factoryName'), '/ds_raw_temp')]",
			"type": "Microsoft.DataFactory/factories/datasets",
			"apiVersion": "2018-06-01",
			"properties": {
				"linkedServiceName": {
					"referenceName": "ls_formula_one_storage",
					"type": "LinkedServiceReference"
				},
				"folder": {
					"name": "formula_one_datasets/raw"
				},
				"annotations": [],
				"type": "DelimitedText",
				"typeProperties": {
					"location": {
						"type": "AzureBlobFSLocation",
						"fileName": "temp.txt",
						"folderPath": "bronze/temp",
						"fileSystem": "bronze"
					},
					"columnDelimiter": ",",
					"escapeChar": "",
					"firstRowAsHeader": false,
					"quoteChar": ""
				},
				"schema": [
					{
						"name": "abcd",
						"type": "String"
					}
				]
			},
			"dependsOn": [
				"[concat(variables('factoryId'), '/linkedServices/ls_formula_one_storage')]"
			]
		},
		{
			"name": "[concat(parameters('factoryName'), '/ds_sink_seasons_data')]",
			"type": "Microsoft.DataFactory/factories/datasets",
			"apiVersion": "2018-06-01",
			"properties": {
				"linkedServiceName": {
					"referenceName": "ls_formula_one_storage",
					"type": "LinkedServiceReference"
				},
				"folder": {
					"name": "formula_one_datasets/raw"
				},
				"annotations": [],
				"type": "Json",
