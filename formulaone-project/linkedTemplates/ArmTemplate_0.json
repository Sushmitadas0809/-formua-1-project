					"location": {
						"type": "AzureBlobFSLocation",
						"fileName": "seasons.json",
						"folderPath": "bronze/seasons",
						"fileSystem": "bronze"
					}
				},
				"schema": {}
			},
			"dependsOn": [
				"[concat(variables('factoryId'), '/linkedServices/ls_formula_one_storage')]"
			]
		},
		{
			"name": "[concat(parameters('factoryName'), '/pl_ingest_drivers_data')]",
			"type": "Microsoft.DataFactory/factories/pipelines",
			"apiVersion": "2018-06-01",
			"properties": {
				"activities": [
					{
						"name": "Lookup_seasons",
						"type": "Lookup",
						"dependsOn": [],
						"policy": {
							"timeout": "0.12:00:00",
							"retry": 0,
							"retryIntervalInSeconds": 30,
							"secureOutput": false,
							"secureInput": false
						},
						"userProperties": [],
						"typeProperties": {
							"source": {
								"type": "JsonSource",
								"storeSettings": {
									"type": "AzureBlobFSReadSettings",
									"recursive": true,
									"enablePartitionDiscovery": false
								},
								"formatSettings": {
									"type": "JsonReadSettings"
								}
							},
							"dataset": {
								"referenceName": "ds_sink_seasons_data",
								"type": "DatasetReference",
								"parameters": {}
							}
						}
					},
					{
						"name": "for_each_seasons",
						"type": "ForEach",
						"dependsOn": [
							{
								"activity": "Lookup_seasons",
								"dependencyConditions": [
									"Completed"
								]
							}
						],
						"userProperties": [],
						"typeProperties": {
							"items": {
								"value": "@activity('Lookup_seasons').output.firstRow.MRData.SeasonTable.Seasons",
								"type": "Expression"
							},
							"batchCount": 20,
							"activities": [
								{
									"name": "Web_for_drivers",
									"type": "WebActivity",
									"dependsOn": [],
									"policy": {
										"timeout": "0.12:00:00",
										"retry": 0,
										"retryIntervalInSeconds": 30,
										"secureOutput": false,
										"secureInput": false
									},
									"userProperties": [],
									"typeProperties": {
										"method": "GET",
										"headers": {},
										"url": {
											"value": "@concat('https://ergast.com/api/f1/',item().season,'/','drivers.json')",
											"type": "Expression"
										}
									}
								},
								{
									"name": "set_drivers_variable",
									"type": "SetVariable",
									"dependsOn": [
										{
											"activity": "Web_for_drivers",
											"dependencyConditions": [
												"Completed"
											]
										}
									],
									"policy": {
										"secureOutput": false,
										"secureInput": false
									},
									"userProperties": [],
									"typeProperties": {
										"variableName": "webs_op",
										"value": {
											"value": "@{activity('Web_for_drivers').output}",
											"type": "Expression"
										}
									}
								},
								{
									"name": "copy_drivers_data",
									"type": "Copy",
									"dependsOn": [
										{
											"activity": "set_drivers_variable",
											"dependencyConditions": [
												"Completed"
											]
										}
									],
									"policy": {
										"timeout": "0.12:00:00",
										"retry": 0,
										"retryIntervalInSeconds": 30,
										"secureOutput": false,
										"secureInput": false
									},
									"userProperties": [],
									"typeProperties": {
										"source": {
											"type": "DelimitedTextSource",
											"additionalColumns": [
												{
													"name": "my_clm",
													"value": {
														"value": "@variables('webs_op')",
														"type": "Expression"
													}
												}
											],
											"storeSettings": {
												"type": "AzureBlobFSReadSettings",
												"recursive": true,
												"enablePartitionDiscovery": false
											},
											"formatSettings": {
												"type": "DelimitedTextReadSettings"
											}
										},
										"sink": {
											"type": "DelimitedTextSink",
											"storeSettings": {
												"type": "AzureBlobFSWriteSettings"
											},
											"formatSettings": {
												"type": "DelimitedTextWriteSettings",
												"quoteAllText": true,
												"fileExtension": ".txt"
											}
										},
										"enableStaging": false,
										"translator": {
											"type": "TabularTranslator",
											"mappings": [
												{
													"source": {
														"name": "my_clm",
														"type": "String"
													},
													"sink": {
														"type": "String",
														"physicalType": "String",
														"ordinal": 1
													}
												}
											],
											"typeConversion": true,
											"typeConversionSettings": {
												"allowDataTruncation": true,
												"treatBooleanAsNumber": false
											}
										}
									},
									"inputs": [
										{
											"referenceName": "ds_raw_drivers_temp",
											"type": "DatasetReference",
											"parameters": {}
										}
									],
									"outputs": [
										{
											"referenceName": "ds_raw_drivers",
											"type": "DatasetReference",
											"parameters": {
												"season": {
													"value": "@item().season",
													"type": "Expression"
												}
											}
										}
									]
								}
							]
						}
					}
				],
				"policy": {
					"elapsedTimeMetric": {}
				},
				"variables": {
					"webs_op": {
						"type": "String"
					}
				},
				"folder": {
					"name": "formula_one_project"
				},
				"annotations": []
			},
			"dependsOn": [
				"[concat(variables('factoryId'), '/datasets/ds_sink_seasons_data')]",
				"[concat(variables('factoryId'), '/datasets/ds_raw_drivers_temp')]",
				"[concat(variables('factoryId'), '/datasets/ds_raw_drivers')]"
			]
		},
		{
			"name": "[concat(parameters('factoryName'), '/pl_ingest_results_data')]",
