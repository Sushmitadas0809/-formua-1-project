{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "formulaone-project"
		},
		"ls_formula_one_storage_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'ls_formula_one_storage'"
		},
		"ls_ergast_api_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://ergast.com/"
		},
		"ls_formula_one_storage_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://formuladlstorage1.dfs.core.windows.net/"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "formulaone-project_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"ls_formula_one_storage_accountKey": {
						"value": "[parameters('ls_formula_one_storage_accountKey')]"
					},
					"ls_ergast_api_properties_typeProperties_url": {
						"value": "[parameters('ls_ergast_api_properties_typeProperties_url')]"
					},
					"ls_formula_one_storage_properties_typeProperties_url": {
						"value": "[parameters('ls_formula_one_storage_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}